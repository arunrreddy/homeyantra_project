{
    "name": "product_demo",
    "version": "0.1.0",
    "description": "Product Demo for homeyantra.com",
    "scripts": {
        "dev": "better-npm-run dev",
        "build": "better-npm-run build",
        "prod": "better-npm-run prod",
        "lint": "eslint --ext .js,.vue src build config",
        "lint:fix": "npm run lint -- --fix",
        "lint-staged": "lint-staged"
    },
    "betterScripts": {
        "dev": {
            "command": "node --inspect dev-nodemon.js",
            "env": {
                "NODE_ENV": "development",
                "DEBUG": "product_demo:*,express:application,express:router:route",
                "DEBUG_DEPTH": 10
            }
        },
        "build": {
            "command": "node build/build.js",
            "env": {
                "NODE_ENV": "production"
            }
        },
        "prod": {
            "command": "node src/app.js",
            "env": {
                "NODE_ENV": "production"
            }
        }
    },
    "lint-staged": {
        "src/**/*.{js,vue}": [
            "eslint --fix"
        ],
        "build/*.js": [
            "eslint --fix",
            "git add"
        ],
        "config/*.js": [
            "eslint --fix",
            "git add"
        ]
    },
    "pre-commit": "lint-staged",
    "dependencies": {
        "babel-polyfill": "^6.23.0",
        "better-npm-run": "0.0.14",
        "bluebird": "^3.5.0",
        "body-parser": "^1.16.0",
        "bunyan": "^1.8.5",
        "debug": "^2.6.0",
        "dot": "^1.1.2",
        "element-ui": "^1.1.5",
        "express": "^4.14.0",
        "express-bunyan-logger": "^1.3.1",
        "joi": "^10.2.0",
        "jsonfile": "^3.0.0",
        "knex": "^0.13.0",
        "lodash": "^4.17.4",
        "mysql2": "^1.3.2",
        "through": "^2.3.8",
        "vue": "^2.1.10",
        "vue-router": "^2.5.3",
        "whatwg-fetch": "^2.0.2"
    },
    "devDependencies": {
        "autoprefixer": "^6.4.0",
        "babel-core": "^6.22.1",
        "babel-eslint": "^7.0.0",
        "babel-loader": "^6.0.0",
        "babel-plugin-component": "^0.9.0",
        "babel-plugin-lodash": "^3.2.11",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-runtime": "^6.22.0",
        "babel-preset-es2015": "^6.22.0",
        "babel-preset-stage-1": "^6.22.0",
        "babel-register": "^6.22.0",
        "chalk": "^1.1.3",
        "compression-webpack-plugin": "^0.3.2",
        "connect-history-api-fallback": "^1.1.0",
        "css-loader": "^0.26.1",
        "eslint": "^3.7.1",
        "eslint-friendly-formatter": "^2.0.5",
        "eslint-loader": "^1.5.0",
        "eslint-plugin-html": "^1.3.0",
        "eslint-plugin-lodash": "^2.3.2",
        "eslint-plugin-promise": "^3.4.0",
        "eventsource-polyfill": "^0.9.6",
        "extract-text-webpack-plugin": "^2.0.0-beta.5",
        "file-loader": "^0.9.0",
        "friendly-errors-webpack-plugin": "^1.1.2",
        "function-bind": "^1.0.2",
        "html-webpack-plugin": "^2.8.1",
        "http-proxy-middleware": "^0.17.2",
        "json-loader": "^0.5.4",
        "lint-staged": "^3.2.8",
        "lodash-webpack-plugin": "^0.11.0",
        "nodemon": "^1.11.0",
        "opn": "^4.0.2",
        "ora": "^1.0.0",
        "pre-commit": "^1.2.2",
        "semver": "^5.3.0",
        "shelljs": "^0.7.4",
        "strip-loader": "^0.1.2",
        "url-loader": "^0.5.7",
        "vue-loader": "^10.0.0",
        "vue-style-loader": "^1.0.0",
        "vue-template-compiler": "^2.1.10",
        "webpack": "^2.2.0",
        "webpack-dev-middleware": "^1.8.3",
        "webpack-hot-middleware": "^2.12.2",
        "webpack-merge": "^2.4.0"
    },
    "engines": {
        "node": ">= 6.9.2",
        "npm": ">= 3.10.9"
    },
    "false": {}
}
